});
var HAP = function () {
    var options = { fixHeader: false, canCommitAnsewer: false, canDownloadAttachment: true, canJoinCompDialog: false, reoadPageAfterCommitAnsewer: false,phase:1 };
    var logined = false;
    var downdialog = null;
    var region;
    var filetype=0;
    var combatTeam = '';
    var isMobile = {
        Android: function () {
            return navigator.userAgent.match(/Android/i);
        },
        BlackBerry: function () {
            return navigator.userAgent.match(/BlackBerry/i);
        },
        iOS: function () {
            return navigator.userAgent.match(/iPhone|iPad|iPod/i);
        },
        Opera: function () {
            return navigator.userAgent.match(/Opera Mini/i);
        },
        Windows: function () {
            return navigator.userAgent.match(/IEMobile/i);
        },
        any: function () {
            return (isMobile.Android() || isMobile.BlackBerry() || isMobile.iOS() || isMobile.Opera() || isMobile.Windows());
        }
    }

    var checkLoginHandle = function (regist) {
        if (typeof (regist) != 'undefined' && regist != null)
            window.location.href = options.server + "/auth/checksso?returnUrl=" + encodeURIComponent(window.location.href) + '&regist=regist';
        else
            //window.location.href = options.server + "/auth/checksso?returnUrl=" + encodeURIComponent(window.location.href) + '&login=login';

            window.location.href = options.server + "/auth/login?returnUrl=" + encodeURIComponent(window.location.href)
    }
    
    var commitAnsewerDialog = null;
    var uploadCommitAnsewer = null;
    var joinCompDialog = null;
    var downloadAttachmentDialog = null;
    var formJoinValidator = null;

    var renderMenuTop = function () {
        if (String(document.title) == "2018HUAWEI软件精英挑战赛") { $(".hor-menu ul li").eq(0).addClass("active"); }
        //if (String(document.title) == "赛事介绍 | HUAWEI软件精英挑战赛") { $(".hor-menu ul li").eq(1).addClass("active"); }
        if (String(document.title) == "赛事介绍 | HUAWEI软件精英挑战赛") { $(".hor-menu ul li").eq(1).addClass("active"); }
        if (String(document.title) == "赛题介绍 | HUAWEI软件精英挑战赛") { $(".hor-menu ul li").eq(2).addClass("active"); }
        if (String(document.title) == "赛事直播 | HUAWEI软件精英挑战赛") { $(".hor-menu ul li").eq(3).addClass("active"); }
        if (String(document.title) == "往期回顾 | HUAWEI软件精英挑战赛") { $(".hor-menu ul li").eq(5).addClass("active"); }
        if (String(document.title) == "我的比赛 | HUAWEI软件精英挑战赛") { $(".hor-menu ul li").eq(6).addClass("active"); }
        if ($(".hor-menu ul li a").eq(6).attr("name") == "我的比赛") {
            if (String(document.title) == "大赛开发指南 | HUAWEI软件精英挑战赛") { $(".hor-menu ul li").eq(7).addClass("active"); }
        }else{
            if (String(document.title) == "大赛开发指南 | HUAWEI软件精英挑战赛") {$(".hor-menu ul li").eq(6).addClass("active");}
        }
        //if (String(document.title) == "个人中心 | HUAWEI软件精英挑战赛") { $(".hor-menu ul li").eq(i).addClass("active"); }
        //if(String(document.title) == "赛事动态 | HUAWEI软件精英挑战赛") { $(".hor-menu ul li").eq(1).addClass("active");}
        //if (String(document.title)== "赛事动态详情 | HUAWEI软件精英挑战赛"){ $(".hor-menu ul li").eq(1).addClass("active");}
        if (String(document.title) == "用例设计 | HUAWEI软件精英挑战赛") { $(".hor-menu ul li").eq(6).addClass("active"); }
        if (String(document.title) == "小组赛 | HUAWEI软件精英挑战赛") { $(".hor-menu ul li").eq(7).addClass("active"); }
        if (String(document.title) == "我的决赛 | HUAWEI软件精英挑战赛") { $(".hor-menu ul li").eq(7).addClass("active"); }

    }

    var checkcanJoin = function () {
        $.ajax({
            url: "/team/checkCanJoin",
            type: "POST",
            dataType: 'json',
            error: function (XMLHttpRequest, textStatus, errorThrown) {
                HAP.showErrorMsgDialog('', '未知网络错误，验证失败！', null);
            },
            success: function (result) {
                if (result.Success) {
                        region = result.Data;
                    joinCompDialog.open();
                    setTimeout(function () {
                        $("#divRegion").html(region);
                    }, 500);
                }
                else {
                    //验证异常
                    if (result.Data == "false") {
                        HAP.showErrorMsgDialog('cannotjoin');
                    } else {

                        HAP.procCLError(result, function () { });
                    }
                }
            }
        });
    }
    var getCombatTeamData = function () {
        $.ajax({
            url: "/team/GetTop38Teams",
            type: "GET",
            dataType: 'json',
            error: function (XMLHttpRequest, textStatus, errorThrown) {
                HAP.showErrorMsgDialog('', '未知网络错误，验证失败！', null);
            },
            success: function (result) {
                var msg = '<option value="-1">---请选择---</option>';
                var result = JSON.parse(result.Data);
                for (var index = 0; index < result.length; index++) {
                    msg += '<option value=' + '"' + result[index].TeamID + '">' + [result[index].Name, result[index].TeamName].join('-') + '</option>'
                }
                $('#combatTeam').html(msg);
            }
        });
    };

    var getRemainUploadTimes = function () {
        $.ajax({
            url: "/team/GetRemainUploadTimes",
            type: "GET",
            dataType: 'json',
            error: function (XMLHttpRequest, textStatus, errorThrown) {
                HAP.showErrorMsgDialog('', '未知网络错误，验证失败！', null);
            },
            success: function (result) {
                var result = JSON.parse(result.Data);
                var RemTimes = result;
                //console.log(RemTimes);
                $('#RemainTimes').html("你的剩余提交次数为 " + RemTimes + " 次");
            }
        });
    };

    var initDialog = function () {
        getRemainUploadTimes();
        var uploadOpt = {
            width: '100%',
            height: 30,
            buttonText: '选择文件',
            'auto': false,
            multi: false,
            'successTimeout': 60,
            'swf': '/static/global/plugin/uploadify/uploadify.swf',
            //'uploader': '/team/uploadanswer',//复赛小组赛之前, 包括：初赛、复活赛
            //'uploader': '/team/uploadanswerrecordr2',//决赛之前，区域现场复赛
            'uploader': '/team/UploadFinalPK',//决赛现场PK赛
            //'uploader': '/team/uploadr2answer',//决赛之前
            //'uploader': '/team/uploadftanswer',
            fileSizeLimit: '10MB',
            queueSizeLimit: 2,
            uploadLimit: 2,
            'formData': { 'ASPSESSID': options.ASPSESSID, 'AUTHID': options.AUTH, 'AnswerType': filetype, 'combatTeam': combatTeam },
            'fileTypeDesc': 'ZIP Files;TAR.GZ Files',
            'fileTypeExts': '*.zip;*.tar.gz',
            //异常处理
            'overrideEvents': ['onSelectError', 'onDialogClose'],
            onSelect: function () {
                var qs = $('#uploadCommitAnsewer-queue .uploadify-queue-item');
                if (qs.length == 2) {
                    $(qs[0]).hide();
                    uploadCommitAnsewer.uploadify('cancel');
                }
            },
            'onSelectError': function (file, errorCode, errorMsg) {
                if (errorCode == -110) {
                    errorMsg = "文件" + file.name + "超过10MB大小限制，请重新上传！"
                    HAP.showErrorMsgDialog('', errorMsg, function () { });
                    return false;
                }
                else if (errorCode == -130) {
                    errorMsg = "请选择zip或tar.gz文件上传！";
                    HAP.showErrorMsgDialog('', errorMsg, function () { });
                    return false;
                } else if (errorCode == -100) {
                    errorMsg = "一次只能上传一个文件！";
                    HAP.showErrorMsgDialog('', errorMsg, function () { });
                    return false;
                }
            },
            'onUploadError': function (file, errorCode, errorMsg, errorString) {
                if (errorCode == -280) {
                    return;
                }
                else if (errorCode == -210) {
                    HAP.showErrorMsgDialog('', '上传路径无效，上传失败（' + errorCode + '）！', function () {
                        HAP.unblockUI();
                    });
                }
                else if (errorCode == -250) {
                    HAP.showErrorMsgDialog('', '未知错误，上传失败（' + errorCode + '）！', function () {
                        HAP.unblockUI();
                    });
                }
                else if (errorCode == -260) {
                    HAP.showErrorMsgDialog('', '无效的文件，上传失败（' + errorCode + '）！', function () {
                        HAP.unblockUI();
                    });
                }
                else if (errorCode == -270) {
                    HAP.showErrorMsgDialog('', '文件校验失败，上传失败（' + errorCode + '）！', function () {
                        HAP.unblockUI();
                    });
                }
                else if (errorCode == -290) {
                    HAP.showErrorMsgDialog('', '上传终止，上传失败（' + errorCode + '）！', function () {
                        HAP.unblockUI();
                    });
                }
                else if (errorCode == -200) {
                    window.location.href = options.server + "/auth/logoff/cn";
                    HAP.showErrorMsgDialog('', '登录超时，请重新登录', function () {
                        HAP.unblockUI();
                    });
                } 
                else {
                    //var msg = 'file:"' + file.name + '" , errorCode:' + errorCode + ', errorMsg:' + errorMsg + ' ,errorString:' + errorString;
                    HAP.showErrorMsgDialog('', '未知的网络错误，上传失败（' + errorCode + '）！', function () {
                        HAP.unblockUI();
                    });
                }
            },
            //'onUploadComplete': function (file) {
            //},
            fileTypeExts: "*.zip;*.tar.gz",
            fileTypeDesc: "zip file; tar.gz file",
            //'onCancel': function (file) {
            //},
            'onUploadStart': function (file) {
                uploadCommitAnsewer.uploadify('settings', 'formData', { 'ASPSESSID': options.ASPSESSID, 'AUTHID': options.AUTH, 'AnswerType': filetype, 'combatTeam':combatTeam })
            },
            'onUploadSuccess': function (file, data, response) {
                commitAnsewerDialog.close();
                HAP.unblockUI();

                data = JSON.parse(data);
                if (data.Success) {
                    //上传成功                    
                    if (options.reoadPageAfterCommitAnsewer) {
                        HAP.showMsgDialog("提交成功！页面正在刷新，请等待......", null, { closable: false });
                        setTimeout(function () {
                            document.location = document.location;
                        }, 2000);
                    }
                    else {
                        //var times = $("#RemainTimes").val();
                        //console.log("Upload:=" + times);
                        getRemainUploadTimes();
                        HAP.showMsgDialog("提交成功！");
                    }
                }
                else {
                    //上传失败
                    //HAP.procCLError(data, function () {
                    //});
                    HAP.showErrorMsgDialog('', data.Message, function () {
                    document.location.reload();
                });
                }
            }
        };
        
        if (options.phase == 2)
        {
            uploadOpt.uploader = '/team/uploadft1answer';
        }
        setTimeout(function () {
            uploadCommitAnsewer = $('#uploadCommitAnsewer').uploadify(uploadOpt);
        }, 50);
    };

    var AddAntiForgeryToken = function (data) {
        data.__RequestVerificationToken = $('#formJoin input[name=__RequestVerificationToken]').val();
        return data;
    };

    var handleValidation = function () {
        var formJoin = $('#formJoin');

        formJoinValidator = formJoin.validate({
            errorElement: 'span', //default input error message container
            errorClass: 'help-block help-block-error', // default input error message class
            focusInvalid: true, // do not focus the last invalid input
            ignore: "",  // validate all fields including form hidden input
            messages: {},
            rules: {
                dTeamName: {
                    minlength: 1,
                    maxlength: 50,
                    required: true
                },
                dTeamCUser: {
                    minlength: 1,
                    maxlength: 50,
                    required: true
                },
                dTeamUser2: {
                    minlength: 1,
                    maxlength: 50
                },
                dTeamUser3: {
                    minlength: 1,
                    maxlength: 50
                },
                dTeamMoto: {
                    minlength:1,
                    maxlength: 500,
                    required: true
                }
            },
            invalidHandler: function (event, validator) { },
            highlight: function (element) { // hightlight error inputs
                $(element)
                    .closest('.form-group').children('div').addClass('has-error'); // set error class to the control group
            },
            unhighlight: function (element) { // revert the change done by hightlight
                $(element)
                    .closest('.form-group').children('div').removeClass('has-error'); // set error class to the control group
            },
            success: function (label) {
                label
                    .closest('.form-group').children('div').removeClass('has-error'); // set success class to the control group
            },
            submitHandler: function (form) {
                try {
                    var data = {
                        TeamName: $('#txtdTeamName').val(),
                        U2: $('#txtdTeamUser2').val(),
                        U3: $('#txtdTeamUser3').val(),
                        TeamDescription: $('#txtdTeamDesc').val(),
                        TeamMoto: $('#txtdTeamMoto').val()
                    };
                    var Cuid = $('#txtdTeamCuser').val();
                    if ((data.U2 != '' && data.U2 == Cuid) || ((data.U3 != '' && data.U3 == Cuid) || (data.U3 != '' && data.U3 == data.U2))) {
                        HAP.unblockUI();
                        HAP.showErrorMsgDialog('', '成员ID重名，请重新填写！', function () {

                        })
                    } else {
                        HAP.blockUI({ message: '数据提交中，请等待......' });

                        $.ajax({
                            url: "/team/joincomp",
                            type: "POST",
                            dataType: 'json',
                            data: AddAntiForgeryToken(data),
                            error: function (XMLHttpRequest, textStatus, errorThrown) {
                                HAP.showErrorMsgDialog('', '登陆失效，请重新登录！', function () {
                                    HAP.unblockUI();
                                });
                            },
                            success: function (result) {
                                HAP.unblockUI();

                                if (result.Success) {
                                    //提交成功
                                    joinCompDialog.close();
                                    HAP.showMsgDialog("数据提交成功。页面正在刷新，请等待......", null, { closable: false });
                                    setTimeout(function () {
                                        document.location = document.location;
                                    }, 2000);
                                }
                                else {
                                    //提交失败                              
                                    HAP.procCLError(result, function () {
                                    });
                                }
                            }
                        });
                    }

                } catch (e) {
                    HAP.showErrorMsgDialog('', '登陆失效，请重新登录！', function () {
                        HAP.unblockUI();
                    });
                }

                return false;
            }
        });
    };
    return {
        //main function to initiate the theme
        init: function (opt) {
            $(".login").click(function () {
                HAP.checkLogin();
            });
            $(".regist").click(function () {
                HAP.checkLogin('regist');
            });
            $(".href-join").click(function (event) {
                stopEvent(event);
                HAP.showjoinComp();
            });

            try {
                if (isMobile.any())
                    $(".hor-menu ul li a:contains(论坛)").attr('href', 'http://forum.huaweicloud.com/forum-649-1.html');
            } catch (e) { }

            $(".href-comanswer").click(function () {
                //var nowdate = new Date();
                //var current = parseInt(nowdate.getHours()) * 60 + parseInt(nowdate.getMinutes());
                //var d1 = parseInt(2) * 60 + parseInt(00);
                //var d2 = parseInt(6) * 60 + parseInt(00);
