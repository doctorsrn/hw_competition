$(function () {
    //监听texarea文本框字数
    var maxLengthValue;
    $("#txtdTeamMoto").bind("input propertychange change", function (event) {
        if ($(this).val().length == 128) {
            maxLengthValue = $(this).val();
        }
        if ($(this).val().length > 128) {
            $(this).val(maxLengthValue);
        }
        $(this).siblings(".textarea-count").find(".count-num").html($(this).val().length);
    });

    $(".btn-cancel").click(function () {
        alert()
        $(".count-num").html("0");
    })
})

function flashChecker() {
    var hasFlash = 0;         //否安装flash  
    var flashVersion = 0; //flash版本  
    var isIE = /*@cc_on!@*/0;      //否IE浏览器  

    if (isIE) {
        var swf = new ActiveXObject('ShockwaveFlash.ShockwaveFlash');
        if (swf) {
            hasFlash = 1;
            VSwf = swf.GetVariable("$version");
            flashVersion = parseInt(VSwf.split(" ")[1].split(",")[0]);
        }
    } else { 
        if (navigator.plugins && navigator.plugins.length > 0) {
            var swf = navigator.plugins["Shockwave Flash"];
            if (swf) {
                hasFlash = 1;
                var words = swf.description.split(" ");
                for (var i = 0; i < words.length; ++i) {
                    if (isNaN(parseInt(words[i]))) continue;
                    flashVersion = parseInt(words[i]);
                }
            }
        }
    }
    return { f: hasFlash, v: flashVersion };
}

var fls = flashChecker();
var s = "";
//if (fls.f) document.write("您安装flash,前flash版本: " + fls.v + ".x");
//else document.write("您没安装flash");

/*校验文件大小格式start*/
function fileSize(field, size) {
    var fileInput = field[0];
    var fileType = field.val().toLowerCase().split(".");
    if (fileType[fileType.length - 1] != "zip" && (fileType[fileType.length - 2] + "." + fileType[fileType.length - 1]) != "tar.gz") {
        HAP.showErrorMsgDialog('', "请选择zip或tar.gz文件上传！", function () { });
        return false;
    } else if (fileInput.files) {
        var byteSize = fileInput.files[0].size;
        var sizeM = Math.ceil(byteSize / 1024 / 1024);
        if (sizeM <= size) {
            return true;
        } else if (sizeM > size) {
            HAP.showErrorMsgDialog('', "超过" + size + "MB大小限制，请重新上传！", function () { });
            return false;
        }
        //else if (sizeM <= 0 && fileType[fileType.length - 1] == "zip") {
        //    alert("文件大小不能为0.");
        //    return false;
        //}
    } else {
        alert("您的浏览器不支持计算文件大小，请确保文件大小小于 " + size + " M，推荐使用ＩＥ9版本以上的浏览器");
        return true;
    }
}
/*校验文件大小格式end*/

function stopEvent(event) { //阻止冒泡事件
    //取消事件冒泡
    var e = arguments.callee.caller.arguments[0] || event; //若省略此句，下面的e改为event，IE运行可以，但是其他浏览器就不兼容
    if (e && e.stopPropagation) {
        // this code is for Mozilla and Opera
        e.stopPropagation();
    } else if (window.event) {
        // this code is for IE
        window.event.cancelBubble = true;
    }
}