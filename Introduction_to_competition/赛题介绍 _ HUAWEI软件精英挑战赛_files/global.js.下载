        procCLError: function (result, callbcak) {
            //报消息
            HAP.showErrorMsgDialog('', result.Message, callbcak);
        },
        blockUI: function (opt) {
            opt = $.extend(true, {}, opt);
            var html = '';
            if (opt.animate) {
                html = '<div class="loading-message ' + (opt.boxed ? 'loading-message-boxed' : '') + '">' + '<div class="block-spinner-bar"><div class="bounce1"></div><div class="bounce2"></div><div class="bounce3"></div></div>' + '</div>';
            } else if (opt.iconOnly) {
                html = '<div class="loading-message' + (opt.boxed ? 'loading-message-boxed' : '') + '"><div class="camera_loader"></div>';
            } else if (opt.textOnly) {
                html = '<div class="loading-message ' + (opt.boxed ? 'loading-message-boxed' : '') + '"><span>&nbsp;&nbsp;' + (opt.message ? opt.message : 'LOADING...') + '</span></div>';
            } else {
                html = '<div class="loading-message ' + (opt.boxed ? 'loading-message-boxed' : '') + '"><div class="camera_loader"></div><span>&nbsp;&nbsp;' + (opt.message ? opt.message : 'LOADING...') + '</span></div>';
            }

            if (opt.target) { // element blocking
                var el = $(opt.target);
                if (el.height() <= ($(window).height())) {
                    opt.cenrerY = true;
                }
                el.block({
                    message: html,
                    baseZ: opt.zIndex ? opt.zIndex : 1200,
                    centerY: opt.cenrerY !== undefined ? opt.cenrerY : false,
                    css: {
                        top: '10%',
                        border: '0',
                        padding: '0',
                        backgroundColor: 'none'
                    },
                    overlayCSS: {
                        backgroundColor: opt.overlayColor ? opt.overlayColor : '#555',
                        opacity: opt.boxed ? 0.05 : 0.5,
                        cursor: 'wait'
                    }
                });
            } else { // page blocking
                $.blockUI({
                    message: html,
                    baseZ: opt.zIndex ? opt.zIndex : 1200,
                    css: {
                        border: '0',
                        padding: '0',
                        backgroundColor: 'none'
                    },
                    overlayCSS: {
                        backgroundColor: opt.overlayColor ? opt.overlayColor : '#555',
                        opacity: opt.boxed ? 0.05 : 0.5,
                        cursor: 'wait'
                    }
                });
            }
        },
        unblockUI: function (target) {
            if (target) {
                $(target).unblock({
                    onUnblock: function () {
                        $(target).css('position', '');
                        $(target).css('zoom', '');
                    }
                });
            } else {
                $.unblockUI();
            }
        },
        fixDialogCol: function () {

        }
    };
}();


