        showErrorMsgDialog: function (type, msg, callback) {
            if (type == 'notlogin') {
                msg = '您还未<a class="href-green href-login">登录</a>或<a class="href-blue href-regist">注册</a>,暂时不能使用此功能！';
            }
            else if (type == 'joined') {
                msg = '您已报名参赛，请返回提交赛题答案！';
            }
            else if (type == 'notjoined') {
                msg = '请先报名参赛！';
            }
            else if (type == "cannotjoin") {
                msg = '请完善<a href="/home/mycenter" class="href-green">个人信息</a>再报名！';
            }else if(type == "devCloudSuccess"){
                msg = '创建成功';
            } else if (type == "devCloudError") {
                //msg = '系统异常，请重试';
            }
            else {
                msg = msg;
            }

            var $message = $('<div class="info-dialog">'
                         + '<div class="info-img"></div>'
                         + '<div class="info-txt">' + msg + '</div>'
                         //+ '<div class="info-tit"><a class="href-gray">返回继续操作</a>，&nbsp;或者<a href="/" class="href-gray">返回首页</a></div>'
                         + '<div class="info-tit"><a class="href-gray">返回继续操作</a></div>'
                         + '</div>');


            var dialog = BootstrapDialog.show({
                title: "提示信息",
                message: $message,
                onhidden: function () {
                    if (callback) callback();
                    //关闭报错刷新
                    location.reload();
                }
            });

            $message.find('.href-gray').click(function () {
                if (msg == "登录超时，请重新登录") {
                    location.reload();
                }
                dialog.close();
            });
            
